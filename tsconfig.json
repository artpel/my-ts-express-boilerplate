{
    "compilerOptions": {
      "outDir": "./dist",
      "target": "es2019",
      "module": "commonjs",
      "lib": ["es2019"],
      "removeComments": true,
      "noUnusedLocals": false,
      "strictNullChecks": false,
      "strict": true,
      "sourceMap": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true
    },
    "include": ["*.ts", "src/**/*.ts"],
    "exclude": ["node_modules", "**/*.d.ts", "dist"]
  }
  